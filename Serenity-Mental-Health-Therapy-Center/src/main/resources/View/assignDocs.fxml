         <font>
            <Font name="Impact" size="20.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
         <effect>
            <DropShadow>
               <input>
                  <Glow />
               </input>
            </DropShadow>
         </effect>
      </Button>
      <Label fx:id="docNameFromCombo" layoutX="424.0" layoutY="138.0" prefHeight="22.0" prefWidth="108.0" text="DocName">
         <font>
            <Font name="Impact" size="16.0" />
         </font>
      </Label>
